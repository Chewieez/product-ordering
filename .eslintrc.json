{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error",4],
        "linebreak-style": ["error","unix"],
        "quotes": ["error","double"],
        "no-console": ["warn"],
        "no-unused-vars": ["off"],
        "no-useless-escape": ["warn"],
        "semi": ["warn","never"]
    }
}